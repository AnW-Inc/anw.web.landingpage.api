{
    "kind": "collectionType",
    "collectionName": "services",
    "info": {
        "singularName": "service",
        "pluralName": "services",
        "displayName": "Service",
        "description": ""
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "name",
            "required": true
        },
        "description": {
            "type": "string",
            "required": false
        },
        "coverImage": {
            "allowedTypes": [
                "images"
            ],
            "type": "media",
            "multiple": false,
            "required": true
        },
        "partners": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::partner.partner",
            "mappedBy": "service"
        }
    }
}
